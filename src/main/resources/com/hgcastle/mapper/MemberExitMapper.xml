<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hgcastle.mapper.MemberExitMapper">
    <update id="exitMembersById" parameterType="com.hgcastle.dto.MemberDTO">
        update tbl_member
        set
        member_quitDate = curDate()
        where
        member_id = #{ id }
        and member_quitDate is not null
    </update>
</mapper>